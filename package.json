{
  "name": "kade",
  "productName": "KADE",
  "version": "1.1.0",
  "description": "A desktop application for Q Methodology",
  "license": "LGPL-3.0",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "NODE_ENV=test jest",
    "package": "electron-forge package",
    "make": "run-s make:mac make:windows",
    "make:mac": "electron-forge make --platform=darwin",
    "make:linux": "electron-forge make --platform=linux --arch=ia32",
    "make:windows": "electron-forge make --platform=win32 --arch=ia32,x64",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "pack": "electron-builder --dir",
    "dist:macOS": "electron-builder --mac",
    "dist:windows": "electron-builder --windows --x64 --ia32",
    "dist:linux": "electron-builder --linux"
  },
  "jest": {
    "runner": "@jest-runner/electron",
    "testEnvironment": "@jest-runner/electron/environment",
    "verbose": false,
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(css|scss|less)$": "jest-css-modules"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/Sections"
    ]
  },
  "keywords": [],
  "author": {
    "name": "Shawn Banasick",
    "email": "ken.q.tools@gmail.com"
  },
  "build": {
    "appId": "com.kade.app",
    "directories": {
      "buildResources": "resources"
    },
    "linux": {
      "category": "Science",
      "description": "KADE is an open source Q methodology app",
      "synopsis": "Q methodology app",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    }
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm",
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "asar": true
      },
      "electronWinstallerConfig": {
        "name": "KADE",
        "noMsi": false
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "KADE"
      }
    }
  },
  "dependencies": {
    "@trodi/electron-splashscreen": "^0.3.1",
    "ag-grid": "^18.1.2",
    "ag-grid-react": "^18.1.0",
    "d3": "^5.5.0",
    "date-fns": "^1.29.0",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.1.0",
    "electron-fetch": "^1.3.0",
    "electron-react-devtools": "^0.5.3",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-manager": "^1.0.6",
    "filesaver.js-npm": "^1.0.1",
    "hoek": "^5.0.3",
    "lodash": "^4.17.19",
    "lru-cache": "^4.1.3",
    "markdown-to-jsx": "^6.6.9",
    "papaparse": "^4.6.0",
    "react": "^16.4.2",
    "react-ace": "^6.1.4",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.2",
    "react-dom-factories": "^1.0.2",
    "react-dropzone": "^4.2.13",
    "react-easy-state": "^5.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-modal": "^3.5.1",
    "react-select": "^2.0.0",
    "react-semantic-toasts": "^0.3.9",
    "react-simpletabs": "^0.7.0",
    "react-styled-select": "^1.1.0",
    "react-toastify": "^4.4.3",
    "react-transition-group": "^2.4.0",
    "reactcss": "^1.2.3",
    "save-svg-as-png": "^1.4.6",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.2",
    "styled-components": "^3.3.3",
    "xlsx": "^0.13.3"
  },
  "devDependencies": {
    "@jest-runner/electron": "^0.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "electron-builder": "^20.38.5",
    "electron-forge": "^5.2.2",
    "electron-installer-debian": "^0.8.1",
    "electron-installer-redhat": "^0.5.0",
    "electron-prebuilt-compile": "2.0.7",
    "electron-wix-msi": "^1.3.0",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3"
  },
  "repository": "https://github.com/shawnbanasick/kade.git"
}
